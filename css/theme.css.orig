/*--------------------------------------------------------------
6.0 Accessibility
--------------------------------------------------------------*/
/* Text meant only for screen readers */
.screen-reader-text {
  clip: rect(1px, 1px, 1px, 1px);
  position: absolute !important;
  height: 1px;
  width: 1px;
  overflow: hidden;
}
.screen-reader-text:hover, .screen-reader-text:active, .screen-reader-text:focus {
  background-color: #f1f1f1;
  background-clip: padding-box;
  /* stops bg color from leaking outside the border: */
  -webkit-border-radius: 3px;
  border-radius: 3px;
  box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.6);
  clip: auto !important;
  color: #21759b;
  display: block;
  font-size: 14px;
  font-size: 1.4rem;
  font-weight: bold;
  height: auto;
  left: 5px;
  line-height: normal;
  padding: 15px 23px 14px;
  text-decoration: none;
  top: 5px;
  width: auto;
  z-index: 100000;
  /* Above WP toolbar */
}
/*--------------------------------------------------------------
9.0 Widgets
--------------------------------------------------------------*/
.widget{
  margin: 0 0 1.2em;
  padding: 0 0 0.5em;
  position: relative;
}
.widget:after{
  position: absolute;
  display: block;
  content: '';
  border-bottom: 2px solid #dbdbdb;
  width: 80%;
  margin: auto;
  left: 0;
  right: 0;
  bottom: -5px;
}
.widget-select{
  max-width: 100%;
}
.widget-main-area{
  margin-top: 21px;
}
.widget-main-area ul{
  list-style: none;
  margin: 0;
}
.widget-main-area ul li{
  margin: 0.1em 0;
  line-height: 1;
  padding: 0 0.6em;
}
.widget-main-area ul li a{
  color: #333;
  font-size: 16px;
}
.widget-main-area ul li a:hover{
  color:#2e84ff;
  text-decoration: none;
}
.widget-title-container{
  padding: 5px 7px;
  font-weight: 500;
  margin-bottom: 10px;
}
.widget-main-area .widget-title{
  font-family: sans-serif;
  margin: 0;
  font-weight: 600;
  font-size: 19px;
  color: #555;
  padding: 2px 7px;
  text-transform: uppercase;
}
/*select tags*/
.widget-main-area section select,
.widget-main-area input[type="search"]{
  font-size: 16px;
  width: 90%;
  border: 1px solid #ddd;
  border-radius: 3px;
  padding: 5px 3px;
  margin: auto;
}
.widget-main-area section select option{
  font-size:16px;
  padding: 5px 3px;
}
.widget-main-area label{
  width: 100%;
}
/* Search widget */
.search-form .search-submit {
  display: none;
}

/*calendar*/
.widget .calendar_wrap table{
  border-collapse: collapse;
  margin: 0 0 1.5em;
  width: 100%;
}
.widget .calendar_wrap caption {
    margin-bottom: 10px;
    font-weight: bold;
    text-align: center;
    font-size: 16px;
}
.widget .calendar_wrap th{
  font-family: 'Roboto', sans-serif;
  font-size: 14px;
  font-size: 1.4rem;
  background: #f8f8f8;
  font-weight: bold;
